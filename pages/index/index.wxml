<!--index.wxml-->
<view class="container">
  <view class="linkBar">
    <view class="item" hover-class="active" bindtap="goscan">
      <view class="imgWrapper">
        <image src="../../img/icon_41.png" class="img"></image>
      </view>
      <view class="txt">入仓</view>
    </view>
    <view class="item" hover-class="active" bindtap="getOut">
       <view class="imgWrapper">
        <image src="../../img/icon_16.png" class="img"></image>
      </view>
      <view class="txt">出仓</view> 
    </view>
    <view class="item" hover-class="active" bindtap="goSearch">
      <view class="imgWrapper">
        <image src="../../img/icon_7.png" class="img"></image>
      </view>
      <view class="txt">查询</view>
    </view>
  </view>
</view>
<dialog id='dialog' dialogTitle="提示" dialogTxt="查无此仓位"></dialog>